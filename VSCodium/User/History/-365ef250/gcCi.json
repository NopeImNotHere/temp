{
  "workbench.colorTheme": "Dracula",
  "editor.fontSize": 18,
  "editor.formatOnSave": true,
  "editor.mouseWheelZoom": true,
  "window.zoomLevel": -1,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "[css]": {
    "editor.suggest.insertMode": "replace"
  },
  "editor.formatOnPaste": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.sortImports": "explicit"
  },
  "workbench.iconTheme": "vscode-icons",
  "tabnine.experimentalAutoImports": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "doxdocgen.generic.authorEmail": "taw.cgh@gmail..com",
  "doxdocgen.generic.authorName": "NopeImNotHere - Timon Wegner",
  "doxdocgen.generic.authorTag": "@author {author} ({email})",
  "doxdocgen.generic.dateFormat": "DD-MM-YYYY",
  "doxdocgen.generic.order": [
    "brief",
    "empty",
    "tparam",
    "param",
    "return",
    "custom",
    "version",
    "author",
    "date",
    "copyright"
  ],
  "doxdocgen.generic.dateTemplate": "@date {date}",
  "doxdocgen.file.copyrightTag": [
    "@copyright Copyright (c) {year}"
  ],
  "terminal.external.windowsExec": "C:\\Users\\timon\\AppData\\Local\\Microsoft\\WindowsApps\\wt.exe",
  "terminal.integrated.fontFamily": "CaskaydiaCove Nerd Font Mono",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorBlinking": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[json]": {},
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "editor.linkedEditing": true,
  "editor.tabSize": 2,
  "files.associations": {
    "*.css": "css"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "git.autofetch": true,
  "typescript.updateImportsOnFileMove.enabled": "always"
}